{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.php?url=$1"
      },
      {
        "src": "/api",
        "dest": "/api/index.php"
      }
    ],
    "regions": ["iad1"],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }